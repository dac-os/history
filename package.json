{
  "name": "dacos-history",
  "version": "0.0.1",
  "description": "historico escolar do sistema da diretoria academica DAC",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha test/ --require blanket -R mocha-spec-cov-alt",
    "predocs": "apidoc -i controllers/ -o docs/",
    "docs": "apidoc-markdown -p docs/ -o README.md",
    "postdocs": "rm -R docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dac-os/history.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dac-os/history/issues"
  },
  "homepage": "https://github.com/dac-os/history",
  "devDependencies": {
    "apidoc-markdown": "^0.1.3",
    "apidoc": "0.5.0",
    "blanket": "^1.1.6",
    "mocha": "^1.20.1",
    "mocha-spec-cov-alt": "^0.1.1",
    "nock": "^0.42.1",
    "should": "^4.0.4",
    "supertest": "^0.13.0"
  },
  "dependencies": {
    "async": "^0.9.0",
    "body-parser": "^1.4.3",
    "express": "^4.6.1",
    "forky": "^0.1.2",
    "mongoose": "^3.8.13",
    "mongoose-json-select": "^0.2.1",
    "nconf": "^0.6.9",
    "slug": "^0.5.0",
    "verror": "^1.4.0",
    "dacos-auth-driver": "dac-os/auth-driver.git",
    "dacos-calendar-driver": "dac-os/calendar-driver.git",
    "dacos-courses-driver": "dac-os/courses-driver.git",
    "dacos-status-driver": "dac-os/status-driver.git",
    "dacos-enrollment-driver": "dac-os/enrollment-driver.git"
  },
  "config": {
    "blanket": {
      "data-cover-never": [
        "node_modules",
        "/test"
      ],
      "pattern": [
        "/controllers",
        "/models",
        "/index.js"
      ]
    }
  }
}
